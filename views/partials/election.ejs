<div class="flex">
  <h5><%=headingTitle %></h5>
  <span id="<%=countName %>" class="ml-2 text-sm px-2 py-1 bg-gray-200"
    ><%=data.length %></span
  >
</div>
<div class="max-w-md mx-auto">
  <% for (let election of data) { %>
  <div class="my-4">
    <h2 class="text-lg font-medium py-2"><%= election.electionName %></h2>
    <p class="text-sm text-gray-500 py-2"><%= election.electionDescription %></p>
    <% if(headingTitle === "New Elections") { %>
    <a
      href="/edit-election/<%= election.id %>"
      class="w-1/5 text-sm bg-teal-500 hover:bg-cyan-600 focus:ring-4 focus:outline-none focus:ring-aqua-300 font-medium rounded-lg text-sm px-5 py-2.5 text-center"
      onclick="editElection(<%= election.id %>)"
    >
      Edit
    </a> &nbsp; &nbsp; &nbsp;
    <a
      href="/editElection"
      class="w-1/5 text-sm bg-teal-500 hover:bg-cyan-600 focus:ring-4 focus:outline-none focus:ring-aqua-300 font-medium rounded-lg text-sm px-5 py-2.5 text-center"
      onclick="editElection(<%= election.id %>)"
    >
      Launch
    </a>
    <% } %>
    <% if(headingTitle === "Completed Elections") { %>
    <a
      href="#"
      class="w-1/5 text-sm bg-teal-500 hover:bg-cyan-600 focus:ring-4 focus:outline-none focus:ring-aqua-300 font-medium rounded-lg text-sm px-5 py-2.5 text-center"
      onclick="editElection(<%= election.id %>)"
    >
      View
    </a> &nbsp; &nbsp; &nbsp;
    <a
      href="#"
      class="w-1/5 text-sm bg-teal-500 hover:bg-cyan-600 focus:ring-4 focus:outline-none focus:ring-aqua-300 font-medium rounded-lg text-sm px-5 py-2.5 text-center"
      onclick="editElection(<%= election.id %>)"
    >
      Results
    </a>
    <% } %>
    <% if(headingTitle === "Active Elections") { %>
    <a
      href="#"
      class="w-1/5 text-sm bg-teal-500 hover:bg-cyan-600 focus:ring-4 focus:outline-none focus:ring-aqua-300 font-medium rounded-lg text-sm px-5 py-2.5 text-center"
      onclick="editElection(<%= election.id %>)"
    >
      View
    </a> &nbsp; &nbsp; &nbsp;
    <a
      href="#"
      class="w-1/5 text-sm bg-teal-500 hover:bg-cyan-600 focus:ring-4 focus:outline-none focus:ring-aqua-300 font-medium rounded-lg text-sm px-5 py-2.5 text-center"
      onclick="editElection(<%= election.id %>)"
    >
      Manage
    </a>
    <% } %>
  </div>
  <% } %>
</div>
