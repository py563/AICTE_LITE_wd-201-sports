<div class="flex">
  <h6
    class="text-base font-bold leading-tight tracking-tight text-gray-900 md:text-lg"
  >
    <%= headingTitle %>
  </h6>
  &nbsp; &nbsp; &nbsp;
  <span id="<%=countName %>" class="ml-2 text-sm px-2 py-1 bg-gray-200"
    ><%=data.length %></span
  >
  &nbsp; &nbsp; &nbsp;
  <a
    href="/addQuestion/<%= electionId %>"
    class="w-1/2 text-blue bg-teal-500 hover:bg-cyan-600 focus:ring-4 focus:outline-none focus:ring-aqua-300 font-medium rounded-lg text-sm px-5 py-2.5 text-center"
  >
    Add New Question
  </a>
</div>
<div class="max-w-md mx-auto">
  <% for (let i = 0; i < data.length; i++) { %>
  <div class="my-4">
    <h2 class="text-lg font-medium"><%= data[i].question.qsText %></h2>
    <p class="text-sm text-gray-500"><%= data[i].question.description %></p>
    <% for (let option of data[i].options) { %>
    <p class="text-sm text-gray-500"><%= option.optText %></p>
    <!-- <% } %> -->
    <button
      type="submit"
      class="w-full text-blue bg-sky-600 hover:bg-cyan-600 focus:ring-4 focus:outline-none focus:ring-aqua-300 font-medium rounded-lg text-sm px-5 py-2.5 text-center"
    >
      Update
    </button>
    <button
      type="submit"
      class="w-full text-blue bg-sky-600 hover:bg-cyan-600 focus:ring-4 focus:outline-none focus:ring-aqua-300 font-medium rounded-lg text-sm px-5 py-2.5 text-center"
    >
      Delete
    </button>
  </div>
  <!-- <% } %> -->
</div>
