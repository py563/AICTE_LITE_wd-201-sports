<div class="flex">
  <h6
    class="text-base font-bold leading-tight tracking-tight text-gray-900 md:text-lg"
  >
    <%=headingTitle %>
  </h6>
  &nbsp; &nbsp; &nbsp;
  <span id="<%=countName %>" class="ml-2 text-sm px-2 py-1 bg-gray-200"
    ><%=data.length %></span
  >
  &nbsp; &nbsp; &nbsp;
  <a
    href="/edit-election/<%= electionId %>/add-voter"
    class="w-1/2 text-blue bg-teal-500 hover:bg-cyan-600 focus:ring-4 focus:outline-none focus:ring-aqua-300 font-medium rounded-lg text-sm px-5 py-2.5 text-center"
  >
    Add New Voter
  </a>
</div>
<div class="max-w-md mx-auto">
  <% for (let voter of data) { %>
  <div class="my-4">
    <p class="text-lg font-medium"><%= voter.vID %> &nbsp; &nbsp; &nbsp;
      <a href="#"
      onclick="deleteVoter(<%= electionId %>, <%= voter.id %>)"
      class="w-1/5 text-blue bg-sky-600 hover:bg-cyan-600 focus:ring-4 focus:outline-none focus:ring-aqua-300 font-medium rounded-lg text-sm px-5 py-2.5 text-center"
      >
        Delete
      </a>
            
    </p>
  </div>
  <% } %>
</div>
